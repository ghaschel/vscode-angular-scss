          end: '\\s*(;|(?=}|\\)))',
          endCaptures: {
            '1': {
              name: 'punctuation.terminator.rule.scss',
            },
          },
          contentName: 'meta.property-value.scss',
          patterns: [
            {
              include: '#general',
